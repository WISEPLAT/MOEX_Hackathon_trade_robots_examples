<clickhouse>
    <keeper_server>
        <tcp_port from_env="CH_KEEPER_TCP_PORT"/>
        <server_id from_env="CH_KEEPER_SERVER_ID"/>
        <log_storage_path>/var/lib/clickhouse/coordination/log</log_storage_path>
        <snapshot_storage_path>/var/lib/clickhouse/coordination/snapshots</snapshot_storage_path>

        <coordination_settings>
            <operation_timeout_ms>10000</operation_timeout_ms>
            <session_timeout_ms>30000</session_timeout_ms>
            <raft_logs_level>warning</raft_logs_level>
        </coordination_settings>

        <raft_configuration>
            <server>
                <id>1</id>
                <hostname>clickhouse-node01</hostname>
                <port from_env="CH_KEEPER_RAFT_SERVER_PORT"/>
            </server>
            <server>
                <id>2</id>
                <hostname>clickhouse-node03</hostname>
                <port from_env="CH_KEEPER_RAFT_SERVER_PORT"/>
            </server>
            <server>
                <id>3</id>
                <hostname>clickhouse-node05</hostname>
                <port from_env="CH_KEEPER_RAFT_SERVER_PORT"/>
            </server>
            <server>
                <id>4</id>
                <hostname>clickhouse-keeper-quorum</hostname>
                <port from_env="CH_KEEPER_RAFT_SERVER_PORT"/>
            </server>
        </raft_configuration>
    </keeper_server>
</clickhouse>